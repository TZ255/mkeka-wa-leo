<!-- Slip 1 -->
<div class="betslipCont" id="betslipCont">
    <div class="cap">
        <h2 id="betslip-ya-leo">
            ⭐⭐⭐ VIP - MKEKA No. 1 ⭐⭐⭐ <br>
            <span class="slip-siku">
                <i class="fa-solid fa-calendar-days"></i>
                <span id="trh_slip" class="trh_slip"></span>
            </span>
        </h2>
    </div>
    <% if (slip1.length < 1) { %>
    <p class="text-center zakesho">* VIP No. 1 bado iko kwenye maandalizi</p>
    <% } %>
    <% if (slip1.length > 1) { %>
    <table class="table">
        <thead>
            <tr class="table-secondary">
                <th scope="col">Betslip</th>
                <th scope="col" class="cell-left">Odds</th>
            </tr>
        </thead>
        <tbody>
            <% for( let mch of slip1 ) { %>
            <tr class="table-light">
                <td>
                    <span style="color: #808080; font-size: 10px; text-decoration: underline;"><%= mch.time %> |
                        <%= mch.league %></span><br>
                    <b class="tip text-success">
                        <%= mch.tip %>
                    </b> <br>
                    <span class="match">
                        <i class="fa-regular fa-futbol"></i>
                        <%= mch.match %>
                    </span>
                </td>
                <td class="cell-left" style="vertical-align: middle;">
                    <%= mch.odd %>
                </td>
            </tr>
            <% } %>

            <tr class="table-secondary">
                <th scope="col" style="font-size: 14px;">
                    Stake: <span class="brown">56,000 TZS</span><br>
                    Possible Win: <span class="brown" id="slipOdd"></span>
                </th>
                <th scope="col" class="cell-left" style="font-size: 14px;">
                    Odds: <span class="brown">
                        <%= slipOdds %>
                    </span> <br>
                    Bonus: <span style="color:green;">+15%</span>
                </th>
            </tr>
        </tbody>
    </table>
    <% } %>
    <div class="bottomSlip">
        <div>Gal Sport Betting</div>
        <div onclick="jiunge()">
            <span>WEKA BET</span>
            <i class="fa-solid fa-up-right-from-square"></i>
        </div>
    </div>
</div>
<br><hr class="my-5"><br>
