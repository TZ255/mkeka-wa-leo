<!-- today -->
<div class="tble-cont" id="lday">
    <div class="table-head">
        <h2>
            <span style="display: block;">⭐⭐ KEKA MIX la Leo ⭐⭐</span>
            <span class="spn_trh"><b><%= jumasiku.leo %>: </b><%= trh.leo %></span>
        </h2>
    </div>
    <table class="table table-dark table-striped-columns">
        <thead>
            <tr>
                <th scope="col">Time</th>
                <th scope="col">Matches</th>
                <th class="tips-th" scope="col">Tips</th>
            </tr>
        </thead>
        <tbody>
            <% stips.forEach(el=> { %>
            <tr itemscope itemtype="https://schema.org/SportsEvent">
                <td class=" time-td">
                    <time itemprop="startDate" datetime="<%= el.startDate %>">
                        <%= el.time %> <br> <span><%= el.siku %></span>
                    </time>
                </td>
                <td class="match-td">
                    <!-- include schemas -->
                    <meta itemprop="name" content="<%= el.match.hm %> vs <%= el.match.aw %> Tips">
                    <meta itemprop="description"
                        content="Utabiri wa leo kati ya <%= el.match.hm %> vs <%= el.match.aw %>">
                    <meta itemprop="performer" content="<%= el.match.hm %> vs <%= el.match.aw %>">
                    <meta itemprop="endDate" content="<%= el.endDate %>">
                    <span class="league" itemprop="location"><%= el.league %></span>
                    <br>
                    <span class="home" itemprop="homeTeam">
                        <%= el.match.hm %>
                    </span>
                    <br>
                    <span class="away" itemprop="awayTeam">
                        <%= el.match.aw %>
                    </span>
                </td>
                <th class="tip-td" style="color: gold;"><%= el.tip %></th>
            </tr>
            <% }) %>
        </tbody>
    </table>
    <% if (stips.length === 0) { %>
        <div class="zakesho">
            * KEKA MIX la <b>Leo: <%= trh.leo %></b> bado liko kwenye maandalizi. Shuka chini kuangalia mikeka mingine iliyotayari.
        </div>
        <% } %>
    <% if (stips.length > 0) { %>
    <%- include('../../0-global/aff.ejs'); %>
    <% } %>
</div>


