<div class="toggle-cont" id="supa-za-leo">
    <div class="toggles">
        <div class="jtoggle" id="jtoggle">¬´ Juzi</div>
        <div class="ytoggle" id="ytoggle">Jana</div>
        <div class="ltoggle active_toggle" id="ltoggle">‚Åï Leo ‚Åï</div>
        <div class="ktoggle" id="ktoggle">Kesho ¬ª</div>
    </div>
</div>

<!-- juzi -->
<div class="tble-cont ficha" id="jday">
    <div class="table-head">
        <h2>‚≠ê‚≠ê‚≠ê Supa Tips za Juzi ‚≠ê‚≠ê‚≠ê</h2>
    </div>
    <table class="table table-dark table-striped-columns">
        <thead>
            <tr>
                <th scope="col">Time</th>
                <th scope="col">Matches</th>
                <th class="tips-th" scope="col">Tips</th>
                <th class="results-th" scope="col">Results</th>
            </tr>
        </thead>
        <tbody>
            <% jtips.forEach(el=> { %>
                <tr>
                    <td class="time-td">
                        <%= el.time %> <br> <span><%= el.siku %></span> 
                    </td>
                    <td class="match-td">
                        <span><%= el.league %></span> <br>
                        <%- el.match.replace(/ - /g, '<br>' ) %>
                    </td>
                    <th class="tip-td" style="color: gold;"><%= el.tip %></th>
                    <td class="matokeo-td"><%= el.matokeo %></td>
                </tr>
                <% }) %>
        </tbody>
    </table>
    <div class="aff">
        <h4><u>PariMatch</u></h4>
        <h4>* bonus ya 150%, weka 10k pata 15k bure ‚úî</h4>
        <h4>* uwezo wa kuedit mkeka uliokosea ‚úî</h4>
        <h4>* cash-out inapatikana muda wote ‚úî</h4>
        <div class="sasa">
            <button onclick="window.open('/pmatch/register')">üèê BONYEZA HAPA KUJISAJILI</button>
            <br>
        </div>
    </div>
</div>

