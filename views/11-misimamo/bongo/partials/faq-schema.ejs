<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": [
          {
            "@type": "Question",
            "name": "Ligi Kuu ya NBC Tanzania Bara in jumla ya timu ngapi?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Ligi kuu ya NBC Tanzania Bara kwa msimu wa 2024/2025 ina jumla ya timu 16 amabapo kati ya hizo, timu inayoshikilia nafasi ya 1 na ya 2 zitashiriki CAF Champions League na timu inayoshikilia nafasi ya 3 itashiriki CAF Confederation Cup."
            }
          },
          {
            "@type": "Question",
            "name": "Nini maana ya Relegation na Relegation Playoff kwenye msimamo wa Ligi Kuu Bara?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Katika msimamo wa ligi kuu bara, 'Relegation' na 'Relegation Play-Off' zinahusiana na kushushwa daraja (au kuepuka kushushwa daraja) kwa timu zilizopo mwishoni mwa msimamo wa ligi.",
              "additionalProperty": [
                {
                  "name": "Relegation",
                  "value": [
                    "Hii inahusu timu mbili zinazoshika nafasi ya chini kabisa kwenye msimamo wa ligi kuu bara kufuatia mwisho wa msimu.",
                    "Timu hizi hushushwa kutoka ligi kuu kwenda ligi daraja la pili (NBC Championship).",
                    "Idadi ya timu zinazoshushwa mara nyingi kwa ligi kuu bara huwa ni timu mbili za mwisho."
                  ]
                },
                {
                  "name": "Relegation Playoff",
                  "value": [
                    "Katika Ligi Kuu, timu zinazoingia kwenye 'Relegation Playoff' mara nyingi ni zile zinazokaribia kushushwa daraja, lakini bado zinapewa nafasi ya mwisho kujinusuru.",
                    "Kwa msimu wa 2024/2025 timu zitakazo kuwa kwenye 'Relegation Playoff' ni timu zitakazo shikilia nafasi ya 13 na 14.",
                    "Hizi timu hushindana dhidi ya timu zinazotaka kupanda daraja kutoka ligi daraja la pili."
                  ]
                }
              ]
            }
          },
          {
            "@type": "Question",
            "name": "Msimu wa 2024/2025 wa NBC Ligi Kuu Bara ulianza lini?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Ratiba ya Ligi Kuu ya NBC Bara 2024/2025 ilianza tarehe 16/08/2024 saa kumi jioni (04:00 PM) kwa mechi ya ufunguzi kati ya Pamba Jiji na Tanzania Prisons iliyoisha kwa sare ya 0:0."
            }
          },
          {
            "@type": "Question",
            "name": "Timu zipi ni vinara wa ligi kuu Tanzania Bara mara kwa mara?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Timu mbili vinara wa mara kwa mara kwa ligi kuu Tanzania Bara ni Young Africans (Yanga SC) na Simba SC.",
              "additionalProperty": [
                {
                  "name": "Mataji ya ligi kuu",
                  "value": [
                    "Young Africans inaongoza kwa mataji (makombe) ya ligi kuu Tanzania ikiwa na jumla ya mataji 30 hadi kufikia mwisho wa msimu wa 2023/2024.",
                    "Simba SC ina jumla ya mataji 22 ya ligi kuu hadi kufikia mwisho wa msimu wa 2023/2024."
                  ]
                }
              ]
            }
          }
        ]
      }
</script>

<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "SportsOrganization",
    "name": "NBC Tanzania Premier League",
    "sport": "Soccer",
    "event": {
      "@type": "SportsEvent",
      "name": "2024/2025 Season Standings",
      "competitor": [
        <% standing.standing.forEach(team => { %>
        {
          "@type": "SportsTeam",
          "name": "<%= team.team.name %>",
          "position": "<%= team.rank %>",
          "additionalProperty": [
            {
              "name": "Matches Played",
              "value": "<%= team.all.played %>"
            },
            {
              "name": "Wins",
              "value": "<%= team.all.win %>"
            },
            {
              "name": "Draws",
              "value": "<%= team.all.draw %>"
            },
            {
              "name": "Losses",
              "value": "<%= team.all.lose %>"
            },
            {
              "name": "Goal Difference",
              "value": "<%= team.goalsDiff %>"
            },
            {
              "name": "Points",
              "value": "<%= team.points %>"
            }
          ]
        }<% if (standing.standing.indexOf(team) !== standing.standing.length - 1) { %>,<% } %>
        <% }); %>
      ]
    }
  }
</script>