<!DOCTYPE html>
<html lang="sw">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description"
        content="Ratiba ya ligi kuu ya NBC Tanzania Bara <%= partials.season_long %>. Angalia ratiba ya msimu mzima wa ligi kuu ya NBC Tanzania bara, matokeo ya ligi kuu bara jana na leo">
    <%- include('../../../0-global/favicon.ejs') %>
    <meta property="article:published_time" content="<%= partials.createdAt %>">
    <meta property="article:modified_time" content="<%= partials.updatedAt %>">
    <link rel="canonical" href="https://mkekawaleo.com/football/fixtures/tanzania/premier-league">
    <title>Ratiba ya Ligi Kuu ya NBC Tanzania Bara <%= partials.season_short %> - Ratiba na Matokeo</title>
    <%- include('../../../0-global/style.ejs'); %>
    <%- include('../../../0-global/google.ejs'); %>
    <%- include('../../../0-global/fonts.ejs'); %>
    <style>
        .tdata {
            font-size: 12px;
        }
        
        .PST {
            text-decoration: line-through;
        }
    </style>
</head>

<body>
    <%- include('../../../0-global/header.ejs'); %>
    <main>
        <section class="left">
            <div class="intro">
                <h1 class="fs-3 p-2">Ratiba ya Ligi Kuu ya NBC Tanzania Bara <%= partials.season_short %> - Ratiba na Matokeo</h1>
                <p class="p-2">Hapa chini tumekuwekea ratiba na matokeo ya Ligi Kuu ya NBC Tanzania Bara kwa msimu mzima wa
                    <%= partials.season_short %>.
                </p>
                <span class="meta-info-el meta-info-date d-block p-2 mb-3 text-muted" style="font-size: 12px;">
                    <i class="fa-regular fa-clock"></i> Updated on:
                    <abbr class="date published" title="<%= partials.updatedAt %>">
                        <%= new Date(partials.updatedAt).toLocaleString('en-US', {timeZone: 'Africa/Nairobi', hour: "2-digit", minute: "2-digit", weekday: "long", year: "numeric", month: "long", day: "numeric",}) %>
                    </abbr>
                </span>
            </div>
            <%- include('./partials/fixtures.ejs'); %>
            <br>
            <div class="vidokezo p-2">
                <h2 class="h2 fs-6">Vidokezo NBC <%= partials.season_short %></h2>
                <ul class="list-group list-group-flush fw-bold">
                    <li class="list-group-item">
                        <a href="/standings/football/tanzania/premier-league" class="text-success text-decoration-none">
                            Msimamo Ligi Kuu Bara (NBC) - Tanzania Premier League <%= partials.season_short %>
                        </a>
                    </li>
                    <li class="list-group-item">
                        <a href="/football/top-scorers/tanzania/premier-league" class="text-success text-decoration-none">
                            Wafungaji Bora NBC - Tanzania Premier League <%= partials.season_short %>
                        </a>
                    </li>
                    <li class="list-group-item">
                        <a href="/football/top-assists/tanzania/premier-league" class="text-success text-decoration-none">
                            Watoa Assists Bora NBC - Tanzania Premier League <%= partials.season_short %>
                        </a>
                    </li>
                </ul>
            </div>
            <br><br>
            <div class="ratiba-timu">
                <h2 class="h2 fs-6">
                    Ratiba na Matokeo kwa Timu Moja Moja - Msimu wa <%= partials.season_long %>
                </h2>
                <ul itemscope itemtype="https://schema.org/BreadcrumbList" class="list-group list-group-flush p-2">
                    <% standing.standing.forEach(team => { %>
                     <li itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem" class="list-group-item">
                        <a itemprop="item" href="/football/fixtures/tanzania/premier-league/<%= team.team.id %>" class="text-success text-decoration-none">
                            <span itemprop="name">
                                Ratiba na Matokeo ya Mechi za <%= team.team.name.replace('Kitayosce', 'Tabora United') %>
                            </span>
                        </a>
                        <meta itemprop="position" content="<%= team.rank %>">
                     </li>
                    <% }) %>
                </ul>
            </div>
            <%- include('../../../0-global/somapia.ejs'); %>
        </section>
        <%- include('../../../0-global/right.ejs'); %>
    </main>

    <%- include('../../../0-global/footer.ejs'); %>

    <%- include('../../../0-global/modal-gsb.ejs'); %>
    <%- include('../../../0-global/script.ejs'); %>

    <!-- bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
</body>

</html>