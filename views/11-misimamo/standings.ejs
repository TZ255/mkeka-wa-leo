<!DOCTYPE html>
<html lang="sw">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description"
        content="Angalia misimamo ya ligi kuu mbalimbali Duniani. Msimamo wa ligi kuu Tanzania, Uingereza, Ujerumani, Uhispania, Italia n.k">
    <link rel="canonical" href="https://mkekawaleo.com/standings">
    <%- include('../0-global/favicon.ejs') %>
    <title>Misimamo ya Ligi Kuu Mbalimbali Duniani <%= jumasiku.mwaka %></title>
    <%- include('../0-global/style.ejs'); %>
    <%- include('../0-global/google.ejs'); %>
    <%- include('../0-global/fonts.ejs'); %>
    <style>
        section.posts .card {
            margin: 12px 16px;
            width: 100%;
            max-width: 360px;
        }

        section.posts .card img {
            width: 100%;
            max-width: 360px;
            height: 206px;
        }
    </style>
</head>

<body>
    <%- include('../0-global/header.ejs'); %>
    <main>
        <section class="leftside">
            <div class="accordion accordion-flush" id="accordionPanelsStayOpenExample">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingOne">
                        <button class="accordion-button fw-bold text-dark" type="button" data-bs-toggle="collapse"
                            data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="true"
                            aria-controls="panelsStayOpen-collapseOne">
                            MISIMAMO YA LIGI KUU 2024/2025
                        </button>
                    </h2>
                    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show"
                        aria-labelledby="panelsStayOpen-headingOne">
                        <div class="accordion-body">
                            <div class="container mt-4">
                                <ul class="list-group list-group-flush">
                                    <li class="list-group-item fw-bold">
                                        <a href="/standings/football/tanzania/premier-league" title="Msimamo wa Ligi Kuu ya NBC Tanzania Bara kwa msimu wa 2024/2025" class="text-decoration-none link-success">
                                            Tanzania - NBC Premier League 2024/2025
                                        </a>
                                    </li>
                                    <% leagues_2024.forEach(league => { %>
                                    <li class="list-group-item fw-bold">
                                        <a href="/standings/football/<%= league.path %>" title="Msimamo wa <%= league.ligi %> - <%= league.name %> kwa msimu wa <%= league.season_long %>" class="text-decoration-none link-success">
                                            <%= league.country %> - <%= league.ligi %> <%= league.season_long %>
                                        </a>
                                    </li>
                                    <% }) %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Next Accordion -->
                <div class="accordion-item mt-5">
                    <h2 class="accordion-header" id="panelsStayOpen-headingTwo">
                        <button class="accordion-button fw-bold text-dark" type="button" data-bs-toggle="collapse"
                            data-bs-target="#panelsStayOpen-collapseTwo" aria-expanded="true"
                            aria-controls="panelsStayOpen-collapseTwo">
                            RATIBA NA MATOKEO YA MECHI
                        </button>
                    </h2>
                    <div id="panelsStayOpen-collapseTwo" class="accordion-collapse collapse show"
                        aria-labelledby="panelsStayOpen-headingTwo">
                        <div class="accordion-body">
                            <div class="container mt-4">
                                <ul class="list-group list-group-flush">
                                    <li class="list-group-item fw-bold">
                                        <a href="/mechi/jana" title="Ratiba na Matokeo ya Mechi Zote za Jana" class="text-decoration-none link-success">
                                            Matokeo ya Mechi za Jana
                                        </a>
                                    </li>
                                    <li class="list-group-item fw-bold">
                                        <a href="/mechi/leo" title="Ratiba na Matokeo ya Mechi Zote za Leo" class="text-decoration-none link-success">
                                            Ratiba na Matokeo ya Mechi za Leo
                                        </a>
                                    </li>
                                    <li class="list-group-item fw-bold">
                                        <a href="/mechi/kesho" title="Ratiba na Matokeo ya Mechi Zote za Kesho" class="text-decoration-none link-success">
                                            Ratiba ya Mechi za Kesho
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br><br><br>
        </section>
    </main>

    <%- include('../0-global/footer.ejs'); %>
    <%- include('../0-global/script.ejs'); %>

    <!-- bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
</body>

</html>